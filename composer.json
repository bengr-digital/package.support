{
    "name": "bengr/support",
    "description": "Bengr support package for providing some repetetive and key functionalitis for project",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "matejkrenek",
            "email": "mate23.krenek@gmail.com"
        }
    ],
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Bengr\\Support\\": "src/",
            "Bengr\\Support\\Tests\\": "tests/",
            "Bengr\\Support\\Database\\Factories\\": "database/factories"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bengr\\Support\\SupportServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^7.13"
    },
    "require": {
        "spatie/laravel-translatable": "^6.5",
        "spatie/laravel-medialibrary": "^10.9",
        "spatie/laravel-package-tools": "^1.15"
    }
}
